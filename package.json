{
  "name": "react-blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://IslamWahid.github.io/react-blog",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.7.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "concurrently --kill-others 'npm run jsonServer' 'react-scripts start'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "jsonServer": "json-server -watch --port 4000 db.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "jest-enzyme": "^7.0.1",
    "json-server": "^0.14.2"
  }
}
